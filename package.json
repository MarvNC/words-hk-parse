{
  "name": "words-hk-parse",
  "homepage": "https://github.com/MarvNC/words-hk-parse",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarvNC/words-hk-parse"
  },
  "version": "1.0.10",
  "description": "Standalone TypeScript library for downloading and parsing Words.hk Cantonese dictionary data",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "test": "bun test",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cantonese",
    "dictionary",
    "words.hk",
    "jyutping",
    "parser",
    "csv"
  ],
  "author": "MarvNC",
  "license": "MIT",
  "dependencies": {
    "@gerhobbelt/xregexp": "^4.4.0-32",
    "axios": "^1.7.9",
    "csv-parser": "^3.0.0",
    "jsdom": "^26.0.0"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "bun-types": "^1.1.42",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "tsdown": "^0.16.5",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  }
}
